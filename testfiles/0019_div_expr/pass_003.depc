func f1(i32_t a, i32_t b) -> i32_t
{
    if (b == 0)
        return 0;
    if (b == -1)
        return -1;
    return a / b;
}
func f2(i32_t a, i32_t b) -> i32_t
{
    if (b == 0)
        return 0;
    if (a == -2147483648)
        return -1;
    return a / b;
}
