func empty_of(typename t) -> array_t(t, 0)
{
    return {};
}

func empty_of_int() -> array_t(i32_t, 0)
{
    return empty_of(i32_t);
}

func empty_of_bool() -> array_t(bool_t, 0)
{
    return empty_of(bool_t);
}
